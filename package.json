{
	"name": "tree-shaking-example",
	"version": "1.0.0",
	"main": "src/index.js",
	"repository": "git@github.com:selfrefactor/tree-shaking-example.git",
	"author": "Dejan Toteff <deyan8284@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"lodash": "^4.17.11",
		"lodash-es": "^4.17.11",
		"rambda": "^2.7.0",
		"rambdax": "^2.7.0",
		"ramda": "^0.26.1",
		"remeda": "^0.0.11"
	},
	"devDependencies": {
		"@babel/core": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"babel-loader": "^8.0.6",
		"console.table": "^0.10.0",
		"filesize": "^4.1.2",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.3",
		"rollup": "^1.12.1",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-node-resolve": "^5.0.0",
		"rollup-plugin-terser": "^4.0.4",
		"terser-webpack-plugin": "^1.2.4",
		"webpack": "^4.31.0",
		"webpack-cli": "^3.3.2"
	},
	"scripts": {
		"clean": "rm -rf .cache parcel rollup webpack",
		"build": "yarn clean && run-s rollup:* webpack:* parcel:*",
		"parcel": "run-s parcel:*",
		"rollup": "run-s rollup:*",
		"webpack": "run-s webpack:*",
		"start": "run-s build showSize",
		"showSize": "node showFileSize",
		"parcel:lodash": "parcel build src/lodash.js --experimental-scope-hoisting -d parcel",
		"parcel:lodash-es": "parcel build src/lodash-es.js --experimental-scope-hoisting -d parcel",
		"parcel:rambda": "parcel build src/rambda.js --experimental-scope-hoisting -d parcel",
		"parcel:rambdax": "parcel build src/rambdax.js --experimental-scope-hoisting -d parcel",
		"parcel:ramda": "parcel build src/ramda.js --experimental-scope-hoisting -d parcel",
		"parcel:remeda": "parcel build src/remeda.js --experimental-scope-hoisting -d parcel",
		"parcel:ramdaBabel": "parcel build src/ramdaBabel.js --experimental-scope-hoisting -d parcel",
		"rollup:lodash": "LIB='lodash' rollup -c",
		"rollup:lodash-es": "LIB='lodash-es' rollup -c",
		"rollup:remeda": "LIB='remeda' rollup -c",
		"rollup:rambdax": "LIB='rambdax' rollup -c",
		"rollup:rambda": "LIB='rambda' rollup -c",
		"rollup:ramda": "LIB='ramda' rollup -c",
		"rollup:ramdaBabel": "LIB='ramdaBabel' rollup -c",
		"webpack:lodash": "LIB='lodash' webpack -p",
		"webpack:lodash-es": "LIB='lodash-es' webpack -p",
		"webpack:rambda": "LIB='rambda' webpack -p",
		"webpack:rambdax": "LIB='rambdax' webpack -p",
		"webpack:remeda": "LIB='remeda' webpack -p",
		"webpack:ramda": "LIB='ramda' webpack -p",
		"webpack:ramdaBabel": "LIB='ramdaBabel' webpack -p"
	}
}
