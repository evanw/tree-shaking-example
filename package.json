{
	"name": "tree-shaking-example",
	"version": "1.0.0",
	"main": "src/index.js",
	"repository": "git@github.com:selfrefactor/tree-shaking-example.git",
	"author": "Dejan Toteff <deyan8284@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"filesize": "^3.5.11",
		"log": "https://github.com/selfrefactor/log#0.6.0",
		"npm-run-all": "^4.1.2",
		"rambda": "^1.0.9",
		"rambdax": "^0.8.8",
		"ramda": "^0.25.0",
		"microbundle": "^0.4.4",
		"uglifyjs-webpack-plugin": "^1.1.6",
		"webpack": "^3.10.0"
	},
	"scripts": {
		"build": "run-p rollup-* webpack-*",
		"start": "run-s build show-file-size",
		"show-file-size": "node showFileSize",
		"webpack-ramda": "webpack --config webpack.config.ramda.js",
		"webpack-rambda": "webpack --config webpack.config.rambda.js",
		"webpack-rambdax": "webpack --config webpack.config.rambdax.js",
		"rollup-ramda": "microbundle --entry src/ramda.js --output ramda",
		"rollup-rambda": "microbundle --entry src/rambda.js --output rambda",
		"rollup-rambdax": "microbundle --entry src/rambdax.js --output rambdax"
	},
	"devDependencies": {}
}
