{
	"name": "tree-shaking-example",
	"version": "1.0.0",
	"main": "src/index.js",
	"repository": "git@github.com:selfrefactor/tree-shaking-example.git",
	"author": "Dejan Toteff <deyan8284@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"babel-core": "^7.0.0-bridge.0",
		"console.table": "^0.10.0",
		"filesize": "^3.6.1",
		"lodash": "^4.17.11",
		"npm-run-all": "^4.1.3",
		"parcel-bundler": "^1.10.3",
		"rambda": "^1.2.6",
		"rambdax": "^0.14.0",
		"ramda": "^0.25.0",
		"remeda": "^0.0.6",
		"rollup": "^0.66.6",
		"rollup-plugin-babel": "^4.1.0-0",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-node-resolve": "^3.4.0",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"webpack": "^4.21.0",
		"webpack-cli": "^3.1.2"
	},
	"scripts": {
		"clean": "rm -rf rollup&&rm -rf webpack&&rm -rf dist",
		"build": "yarn clean&&run-p rollup:* webpack:* parcel:*",
		"parcel": "run-p parcel:*",
		"rollup": "run-p rollup:*",
		"webpack": "run-p webpack:*",
		"start": "run-s build show:size",
		"show:size": "node showFileSize",
		"parcel:lodash": "parcel build src/lodash.js --target node --out-file lodash.js",
		"parcel:rambda": "parcel build src/rambda.js --target node --out-file rambda.js",
		"parcel:rambdax": "parcel build src/rambdax.js --target node --out-file rambdax.js",
		"parcel:ramda": "parcel build src/ramda.js --target node --out-file ramda.js",
		"parcel:remeda": "parcel build src/remeda.js --target node --out-file remeda.js",
		"parcel:ramdaBabel": "parcel build src/ramdaBabel.js --target node --out-file ramdaBabel.js",
		"rollup:lodash": "LIB='lodash' rollup -c",
		"rollup:remeda": "LIB='remeda' rollup -c",
		"rollup:rambdax": "LIB='rambdax' rollup -c",
		"rollup:rambda": "LIB='rambda' rollup -c",
		"rollup:ramda": "LIB='ramda' rollup -c",
		"rollup:ramdaBabel": "LIB='ramdaBabel' rollup -c",
		"webpack:lodash": "LIB='lodash' webpack",
		"webpack:rambda": "LIB='rambda' webpack",
		"webpack:rambdax": "LIB='rambdax' webpack",
		"webpack:remeda": "LIB='remeda' webpack",
		"webpack:ramda": "LIB='ramda' webpack",
		"webpack:ramdaBabel": "LIB='ramdaBabel' webpack"
	},
	"devDependencies": {}
}
